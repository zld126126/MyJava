<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>

<head>
#showHead ("Petstore Sample")
<link href="$homeContent.getURI("css/petstore-common.css")" rel="stylesheet" type="text/css">
<link href="$homeContent.getURI("css/petstore-homepage.css")" rel="stylesheet" type="text/css">
</head>

<body #bodyAttributes ()>

<table border="0" cellpadding="0" cellspacing="0" width="100%">
  <tr>
    <td height="258" valign="top"><table border="0" cellpadding="0" cellspacing="0" width="100%">
      <tr>
        <td><a href="$petstoreHomeLink"><img src="$homeContent.getURI("images/logo_petstore_big.png")" /></a></td>
      </tr>
		      <tr>
        <td valign="top">&nbsp;</td>
      </tr>
      <tr>
        <td valign="top">$control.setTemplate("user:login")</td>
      </tr>
    </table></td>
    <td><img src="$homeContent.getURI("images/dot_transparent.png")" width="30"/></td>
    <td width="100%" valign="top">
      $control.setTemplate("topNoLogo")
      <table width="100%"  border="0" cellspacing="0" cellpadding="0">
        <tr valign="bottom">
          <td width="10" align="left">$control.setTemplate("tabs")</td>
        </tr>
        <tr>
          <td><table border="0" cellpadding="0" cellspacing="10" width="100%" class="box">
              <tr align="left">
                <td width="100%">$screen_placeholder</td>
              </tr>
            </table></td>
        </tr>
      </table></td>
  </tr>
  <tr>
    <td colspan="3" valign="top">$control.setTemplate("bottom")</td>
  </tr>
</table>

</body>
</html>
